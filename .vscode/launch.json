{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
      {
        "type": "java",
        "name": "ServerMain",
        "request": "launch",
        "mainClass": "com.anwithayi.Server.ServerMain",
        "projectName": "gooseing_game"
      },
      {
        "name": "Launch Chrome",
        "request": "launch",
        "type": "chrome",
        "url": "http://127.0.0.1:8082/frontend/index.html",
        "webRoot": "${workspaceFolder}"
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Program",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "program": "${workspaceFolder}/frontend/script.js"
      }
    ]
}
